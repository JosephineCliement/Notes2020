11.
#include<stdio.h>
#include<conio.h>

int main()
{
	printf("%d\n",(5/2));
	
	printf("%f\n",(5.4/2));
	
	printf("%f\n",(5/2.1));
	
	printf("%f\n",(5.4/2.1));
	
	float f=5/2.0;
	
	printf("%f",f);
	
	return 0;
}
