13
#include<stdio.h>
#include<conio.h>

int main()
{
	int number;
	
	int number1=500;  // compile time initialization
	
	printf("Enter any number : ");
	scanf("%d",&number);        // run time initialization
	
	printf("You entered number : %d\n",number);
	printf("number1 : %d",number1);
	
	return 0;
}

