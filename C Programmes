16

#include<stdio.h>
#include<conio.h>

int main()
{
	int number;
	char alpha;
	float pi;
	
	printf("Enter number : ");
	scanf("%d",&number);
	
	fflush(stdin);
	printf("Enter alphabet : ");
	scanf("%c",&alpha);
	fflush(stdin);
	
	printf("Enter pi : ");
	scanf("%f",&pi);
	
	printf("Number : %d\t Alphabet : %c\tpi : %f\n",number,alpha,pi);
	return 0;
}

